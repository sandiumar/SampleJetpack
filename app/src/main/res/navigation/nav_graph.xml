<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@+id/launcher_fragment">

    <fragment
        android:id="@+id/launcher_fragment"
        android:name="id.sandyu.samplejetpack.ui.main.MainFragment"
        android:label="main_fragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_aboutFragment"
            app:destination="@+id/about"
            app:enterAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/about"
        android:name="id.sandyu.samplejetpack.ui.main.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about">
    </fragment>
</navigation>


